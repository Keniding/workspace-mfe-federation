{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@workspace-mfe-federation/atoms": [
        "apps/angular/libs/shared/ui/atoms/src/index.ts"
      ],
      "@workspace-mfe-federation/molecules": [
        "apps/angular/libs/shared/ui/molecules/src/index.ts"
      ],
      "@workspace-mfe-federation/organisms": [
        "apps/angular/libs/shared/ui/organisms/src/index.ts"
      ],
      "appointment/Routes": [
        "apps/angular/plugins/features/appointment/src/app/remote-entry/entry.routes.ts"
      ],
      "footer/Routes": [
        "apps/angular/plugins/layout/footer/src/app/remote-entry/entry.routes.ts"
      ],
      "header/Routes": [
        "apps/angular/plugins/layout/header/src/app/remote-entry/entry.routes.ts"
      ],
      "hero/Routes": [
        "apps/angular/plugins/layout/hero/src/app/remote-entry/entry.routes.ts"
      ],
      "iaconsultant/Routes": [
        "apps/angular/plugins/features/iaconsultant/src/app/remote-entry/entry.routes.ts"
      ],
      "landing/Routes": [
        "apps/angular/plugins/features/landing/src/app/remote-entry/entry.routes.ts"
      ],
      "navbar/Routes": [
        "apps/angular/plugins/layout/navbar/src/app/remote-entry/entry.routes.ts"
      ],
      "ownerdashboard/Routes": [
        "apps/angular/plugins/dashboards/ownerdashboard/src/app/remote-entry/entry.routes.ts"
      ],
      "petprofile/Routes": [
        "apps/angular/plugins/features/petprofile/src/app/remote-entry/entry.routes.ts"
      ],
      "vetdashboard/Routes": [
        "apps/angular/plugins/dashboards/vetdashboard/src/app/remote-entry/entry.routes.ts"
      ],

      "@workspace-mfe-federation/domain/interfaces/*": [
        "apps/angular/libs/domain/interfaces/*"
      ],
      "@workspace-mfe-federation/core/plugin-registry/*": [
        "apps/angular/libs/core/plugin-registry/*"
      ],
    }
  },
  "exclude": ["node_modules", "tmp"]
}
